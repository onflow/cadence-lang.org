{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/docs/cadence_migration_guide/:path*",
      "destination": "/docs/cadence-migration-guide/:path*",
      "permanent": true
    },
    {
      "source": "/docs/1.0/cadence_migration_guide/:path*",
